#include <stdio.h>
#include <string.h>

int main()
{
    int t, i;
    long long k=0, sum=0;
    char a[10], c[10];

    scanf("%d", &t);
    for(i=0; i<t; i++)

        {
            scanf("%s", a);
            if(!strcmp(a, "donate"))
            {
                scanf("%d", &k);
                sum=sum+k;
            }
            else if(!strcmp(a, "report"))

               {
                   printf("%lld\n", sum);
               }
        }
    return 0;
}
