#include <stdio.h>
#include <string.h>

int main()
{
    char input[10];
    int c = 1;

    while(scanf("%s", input))
    {
        if(!strcmp(input, "*")) break;
        else if(!strcmp(input, "Hajj")) printf("Case %d: Hajj-e-Akbar\n", c++);
        else if(!strcmp(input, "Umrah")) printf("Case %d: Hajj-e-Asghar\n", c++);
    }
    return 0;
}
