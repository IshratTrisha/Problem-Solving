#include <stdio.h>
#include <stdlib.h>

int main()
{
    int t, n, i, j, p[21];
    int captain;

    scanf("%d", &t);

    for(i=1; i<=t; i++)
    {
        scanf("%d", &n);
        for(j=0; j<n; j++)
        {
            scanf("%d", &p[j]);
            if(j==n/2) captain = p[j];
        }
        printf("Case %d: %d\n", i, captain);
    }
    return 0;
}
