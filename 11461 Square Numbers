#include <stdio.h>
#include <stdlib.h>

int main()
{
    long long a, b, c, d, i;


    while(scanf("%lld %lld", &a, &b)==2)
    {
        if(a==0 && b==0) break;
        d =0;
        for(i=a; i<=b; i++)
        {
        c = sqrt(i);
        if(c*c==i)
            d++;
        }
        printf("%lld\n", d);
    }
    return 0;
}
