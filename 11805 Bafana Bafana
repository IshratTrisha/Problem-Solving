#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a, b, c, sum, t, i;

    while(scanf("%d", &t)==1)
    {
        for(i=1; i<=t; i++)
        {
            scanf("%d %d %d", &a, &b, &c);
            sum = b+c;

            while(sum>a)
            {
                sum = sum-a;
            }
            printf("Case %d: %d\n", i, sum);
        }
    }
    return 0;
}
