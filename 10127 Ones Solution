#include<stdio.h>
int main()
{
int a, b, c, d;

while(scanf("%d", &a)==1)
{
    b=1;
    c=1;
    d=1;

    while(1)
    {
        if(a==0)
        {

            printf("0\n");
            break;
        }
        else if(a==1)
        {
            printf("1\n");
            break;
        }
        else if(a==111)
        {
            printf("3\n");
            break;
        }
        else if(a==11)
        {
            printf("2\n");
            break;
        }
        else if(a==1111)
        {
            printf("4\n");
            break;
        }
        c = d%a;
        if(c==0)
        {
            printf("%d\n", b);
            break;
        }
        d = c*10+1;
        b++;
    }
}
    return 0;
}
